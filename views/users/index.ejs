<!DOCTYPE html>
<html>
  <head>
    <title>Lista de Usuarios</title>
  </head>
  <body>
    <h1>Lista de Usuarios</h1>
    <table>
      <thead>
        <tr>
          <th>Nombre de usuario</th>
          <th>Email</th>
          <th>Contraseña</th>
          <th>Pais</th>
        </tr>
      </thead>
      <tbody>
        <% users.forEach(function(user) { %>
        <tr>
          <td><%= user.nombre_usuario %></td>
          <td><%= user.email %></td>
          <td><%= user.contraseña %></td>
          <td><%= user.pais %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </body>
</html>
